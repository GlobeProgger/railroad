<h1>Listing articles</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
	<td><%= link_to "article #{article.id}", article_path(article)%></td>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
    </tr>
  <% end %>
</table>
<button>
<%= link_to "New Article", new_article_path(@articles)%>
</button>
